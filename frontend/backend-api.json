  "info": {
    "title": "ReviewTime",
    "description": "The ReviewTime main API",
    "version": "0.0.1",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "AuthorizeResponse": {
        "type": "object",
        "properties": {
          "authorization_url": {
            "type": "string"
          }
        },
        "required": [
          "authorization_url"
        ]
      },
      "User": {
        "type": "object",
        "properties": {
          "login": {
            "type": "string"
          },
          "avatarUrl": {
            "type": "string"
          }
        },
        "required": [
          "login",
          "avatarUrl"
        ]
      },
      "ReviewRequest": {
        "type": "object",
        "properties": {
          "dueAt": {
            "format": "date-time",
            "type": "string"
          },
          "reviewer": {
            "$ref": "#/components/schemas/User"
          }
        },
        "required": [
          "dueAt",
          "reviewer"
        ]
      },
      "Repository": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          }
        },
        "required": [
          "name"
        ]
      },
      "AuthoredPullRequest": {
        "type": "object",
        "properties": {
          "reviewRequests": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ReviewRequest"
            }
          },
          "title": {
            "type": "string"
          },
          "url": {
            "type": "string"
          },
          "repository": {
            "$ref": "#/components/schemas/Repository"
          }
        },
        "required": [
          "reviewRequests",
          "title",
          "url",
          "repository"
        ]
      },
      "AuthoredByUserResponse": {
        "type": "object",
        "properties": {
          "pullRequests": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AuthoredPullRequest"
            }
          }
        },
        "required": [
          "pullRequests"
        ]
      },
      "ReviewingPullRequest": {
        "type": "object",
        "properties": {
          "author": {
            "$ref": "#/components/schemas/User"
          },
          "reviewDueAt": {
            "format": "date-time",
            "type": "string"
          },
          "title": {
            "type": "string"
          },
          "url": {
            "type": "string"
          },
          "repository": {
            "$ref": "#/components/schemas/Repository"
          }
        },
        "required": [
          "author",
          "reviewDueAt",
          "title",
          "url",
          "repository"
        ]
      },
      "ReviewRequestedFromUserResponse": {
        "type": "object",
        "properties": {
          "pullRequests": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ReviewingPullRequest"
            }
          }
        },
        "required": [
          "pullRequests"
        ]
      },
      "UserOverviewResponse": {
        "type": "object",
        "properties": {
          "waitingForOthers": {
            "type": "number"
          },
          "waitingForUser": {
            "type": "number"
          },
          "nextReviewDue": {
            "$ref": "#/components/schemas/ReviewingPullRequest"
          }
        },
        "required": [
          "waitingForOthers",
          "waitingForUser"
        ]
      },
      "LoggedInUserResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "login": {
            "type": "string"
          },
          "avatar_url": {
            "type": "string"
          },
          "installations": {
            "type": "array",
            "items": {
              "type": "number"
            }
          }
        },
        "required": [
          "id",
          "login",
          "avatar_url",
          "installations"
        ]
      },
      "SettingsResponse": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "emailNotificationsEnabled": {
            "type": "boolean"
          }
        },
        "required": [
          "emailNotificationsEnabled"
        ]
      },
      "UpdateEmailRequest": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "emailNotificationsEnabled": {
            "type": "boolean"
          }
        },
        "required": [
          "email",
          "emailNotificationsEnabled"
        ]
      }
    }
  }
}